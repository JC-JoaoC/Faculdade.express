  <head>
    <title>Agenda de Contatos</title>
    <!-- CONEXÃ•ES -->
    <link rel="stylesheet" href="cadastro/contatos.css" />
    <script src="cadastro/main.js" defer></script>
    <script src="https://unpkg.com/@phosphor-icons/web"></script>
  </head>
  <body>
    <main class="container">
      <header class="header">
        <h1>Agenda de Contatos</h1>
      </header>

      <form id="form-agenda">
        <input
          type="text"
          placeholder="Digite a URL da foto"
          id="foto-contato"
        />
        <input
          type="text"
          placeholder="Digite o nome do contato"
          required
          id="nome-contato"
        />
        <input
          type="tel"
          placeholder="(xx) xxxxx-xxxx"
          required
          id="tel-contato"
          pattern="[0-9]{10,11}"
        />
        <button type="submit">
          <i class="ph ph-address-book-tabs"></i>
          Adicionar
        </button>
        <button type="button" onclick="limparTabela()">
          <i class="ph ph-trash"></i>
          Limpar lista
        </button>
      </form>

      <table cellspacing="0">
        <thead>
          <tr>
            <th>Foto</th>
            <th>Nome contato</th>
            <th>Telefone</th>
          </tr>
        </thead>
        <tbody>
          <!-- HTML gerado via JavaScript -->
        </tbody>
        <tfoot>
          <tr>

          </tr>
        </tfoot>
      </table>
    </main>
  </body>
</html>